{
  "name": "blog-bookmarks-manager",
  "title": "Blog Bookmarks Manager",
  "description": "Manage your blog's bookmarks.json file",
  "icon": "bookmark.png",
  "author": "Waleed",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "list-bookmarks",
      "title": "List Bookmarks",
