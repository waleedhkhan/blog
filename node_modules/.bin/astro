#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/waleed.hussain/code/blog/node_modules/.pnpm/astro@5.1.1_@types+node@20.17.14_jiti@2.0.0-beta.3_rollup@4.30.1_tsx@4.19.2_typescript@5.7.3_yaml@2.7.0/node_modules/astro/node_modules:/Users/waleed.hussain/code/blog/node_modules/.pnpm/astro@5.1.1_@types+node@20.17.14_jiti@2.0.0-beta.3_rollup@4.30.1_tsx@4.19.2_typescript@5.7.3_yaml@2.7.0/node_modules:/Users/waleed.hussain/code/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/waleed.hussain/code/blog/node_modules/.pnpm/astro@5.1.1_@types+node@20.17.14_jiti@2.0.0-beta.3_rollup@4.30.1_tsx@4.19.2_typescript@5.7.3_yaml@2.7.0/node_modules/astro/node_modules:/Users/waleed.hussain/code/blog/node_modules/.pnpm/astro@5.1.1_@types+node@20.17.14_jiti@2.0.0-beta.3_rollup@4.30.1_tsx@4.19.2_typescript@5.7.3_yaml@2.7.0/node_modules:/Users/waleed.hussain/code/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
