#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/waleed/code/blog/node_modules/.pnpm/astro@5.1.1_@types+node@22.10.5_jiti@2.0.0-beta.3_lightningcss@1.28.2_rollup@4.30.1_terser@5._4ifv2w7ezrbmgxhixwsteqr5we/node_modules/astro/node_modules:/Users/waleed/code/blog/node_modules/.pnpm/astro@5.1.1_@types+node@22.10.5_jiti@2.0.0-beta.3_lightningcss@1.28.2_rollup@4.30.1_terser@5._4ifv2w7ezrbmgxhixwsteqr5we/node_modules:/Users/waleed/code/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/waleed/code/blog/node_modules/.pnpm/astro@5.1.1_@types+node@22.10.5_jiti@2.0.0-beta.3_lightningcss@1.28.2_rollup@4.30.1_terser@5._4ifv2w7ezrbmgxhixwsteqr5we/node_modules/astro/node_modules:/Users/waleed/code/blog/node_modules/.pnpm/astro@5.1.1_@types+node@22.10.5_jiti@2.0.0-beta.3_lightningcss@1.28.2_rollup@4.30.1_terser@5._4ifv2w7ezrbmgxhixwsteqr5we/node_modules:/Users/waleed/code/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
